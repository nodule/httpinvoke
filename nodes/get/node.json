{
  "name":"get",
  "ns": "httpinvoke",
  "description":"A HTTP get request",
  "phrases": {
    "active": "Requesting url {{input.url}}"
  },
  "require":{
    "httpinvoke":"1.x.x"
  },
  "ports":{
    "input":{
      "url":{
        "type":"string",
        "format":"url",
        "title":"Url Request",
        "description":"The url to be requested.",
        "required": true
      }
    },
    "output":{
      "error":{
        "title": "Error",
        "type":"object"
      },
      "body":{
        "title": "Body",
        "type":"string"
      },
      "statusCode":{
        "title": "Status Code",
        "type":"string"
      },
      "headers":{
        "title": "Reply Headers",
        "type":"object"
      }
    }
  }
}
