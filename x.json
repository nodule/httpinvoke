{
  "name": "httpinvoke",
  "description": "HTTP Invoke Nodule",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/httpinvoke.git"
  },
  "dependencies": {
    "httpinvoke": "latest"
  },
  "nodes": [
    {
      "dependencies": {
        "npm": {
          "httpinvoke": "1.x.x"
        }
      },
      "ports": {
        "input": {
          "url": {
            "type": "string",
            "format": "url",
            "title": "Url Request",
            "description": "The url to be requested.",
            "required": true
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "body": {
            "title": "Body",
            "type": "string"
          },
          "statusCode": {
            "title": "Status Code",
            "type": "string"
          },
          "headers": {
            "title": "Reply Headers",
            "type": "object"
          }
        }
      },
      "phrases": {
        "active": "Requesting url {{input.url}}"
      },
      "description": "A HTTP get request",
      "ns": "httpinvoke",
      "name": "get",
      "fn": "var obj = {};\nobj.httpinvoke = httpinvoke;\n//output = [obj, 'httpinvoke', input.url, 'GET', input.options];\noutput = [obj, 'httpinvoke', input.url, 'GET'];\n"
    }
  ],
  "twigs": []
}